{
  "name": "ng-apimock",
  "description": "Node plugin that provides the ability to use scenario based api mocking of angular apps",
  "version": "1.4.4",
  "homepage": "https://mdasberg.github.io/ng-apimock",
  "author": {
    "name": "Mischa Dasberg",
    "email": "mischa@dasberg.nl"
  },
  "contributors": [
    "Justus Romijn <justus.romijn@gmail.com>",
    "Richard Rijnberk <r.rijnberk@gmail.com>",
    "Frank Merema <frankmerema@gmail.com>",
    "Jeroen van de Pol <jeroenvandepol@gmail.com>",
    "Wim Selles <wim@wswebcreation.nl>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdasberg/ng-apimock.git"
  },
  "bugs": {
    "url": "https://github.com/mdasberg/ng-apimock/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/mdasberg/ng-apimock/blob/master/LICENSE-MIT"
    }
  ],
  "main": "tasks/index.js",
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "verify": "tslint -p tsconfig.json",
    "verify-type-check": "npm run verify -- --type-check",
    "local": "jasmine-node test/unit/*.spec.js && protractor test/protractor/config/protractor-local-interface.conf.js && protractor test/protractor/config/protractor-local-protractor.conf.js",
    "travis-interface": "protractor test/protractor/config/protractor-travis-interface.conf.js",
    "travis-protractor": "protractor test/protractor/config/protractor-travis-protractor.conf.js",
    "release": "npm publish && node dockerize.js && docker build --file=docker/Dockerfile docker -t mdasberg/ng-apimock && docker push mdasberg/ng-apimock",
   
    "whitebox": "jasmine-ts \"src/**/*.spec.ts\"",
    "protractor-angular-local": "protractor test/interfaces/protractor/angular/protractor.local.conf.js",
    "protractor-angularjs-local": "protractor test/interfaces/protractor/angularjs/protractor.local.conf.js",
    "protractor-angular-travis": "protractor test/interfaces/protractor/angular/protractor.travis.conf.js",
    "protractor-angularjs-travis": "protractor test/interfaces/protractor/angularjs/protractor.travis.conf.js",
    "wdio-local": "wdio test/interfaces/webdriverio/wdio.local.conf.js",
    "wdio-travis": "wdio test/interfaces/webdriverio/wdio.travis.conf.js",
    "blackbox-local": "npm run protractor-angular-local && npm run protractor-angularjs-local && npm run wdio-local",
    "blackbox-travis": "npm run protractor-angular-travis && npm run protractor-angularjs-travis && npm run wdio-travis"
  },
  "dependencies": {
    "angular": "1.6.9",
    "angular-mocks": "1.6.9",
    "angular-resource": "1.6.9",
    "async": "2.6.0",
    "chokidar": "2.0.1",
    "fs-extra": "5.0.0",
    "glob": "7.1.2",
    "hooker": "0.2.3",
    "inversify": "4.10.0",
    "lodash": "4.17.5",
    "path": "0.12.7",
    "reflect-metadata": "0.1.12",
    "sinon": "4.1.4",
    "then-request": "5.0.0",
    "url-join": "4.0.0",
    "uuid": "3.2.1"
  },
  "keywords": [
    "angular",
    "api",
    "mock",
    "scenario",
    "httpbackend"
  ],
  "peerDependencies": {
    "protractor": "5.x"
  },
  "devDependencies": {
    "@types/async": "2.0.47",
    "@types/chokidar": "1.7.4",
    "@types/connect": "3.4.31",
    "@types/fs-extra": "5.0.0",
    "@types/glob": "5.0.35",
    "@types/jasmine": "2.8.6",
    "@types/node": "8.9.1",
    "@types/sinon": "4.1.3",
    "@types/webdriverio": "4.8.8",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "connect": "3.6.5",
    "cucumber": "4.0.0",
    "cucumberjs-junitxml": "1.0.0",
    "jasmine-node": "1.14.5",
    "jasmine-ts": "0.2.1",
    "ng-apimock-angular-test-app": "1.0.1",
    "ng-apimock-angularjs-test-app": "1.0.2",
    "protractor": "5.3.0",
    "protractor-cucumber-framework": "4.2.0",
    "serve-static": "1.13.2",
    "shortid": "2.2.8",
    "sinon": "4.2.2",
    "tslint": "5.9.1",
    "tslint-eslint-rules": "4.1.1",
    "typescript": "2.7.1",
    "vrsource-tslint-rules": "5.8.0",
    "wdio-cucumber-framework": "1.1.0",
    "wdio-selenium-standalone-service": "0.0.9",
    "webdriverio": "4.10.2"
  }
}
